{
    "name": "cvilleger/geo-gouv",
    "description": "Simple Geo Gouv Library. Query geographic reference data easily.",
    "homepage": "https://github.com/cvilleger/geo-gouv",
    "license": "MIT",
    "type": "library",
    "keywords": [
        "geo",
        "gouv"
    ],
    "authors": [
        {
            "name": "Christophe Villeger",
            "email": "cvilleger@darkmira.fr"
        }
    ],
    "require": {
        "php": "^8.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^12.5",
        "friendsofphp/php-cs-fixer": "^3.92",
        "phpstan/phpstan": "^2.1",
        "vimeo/psalm": "^6.14",
        "rector/rector": "^2.3"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Cvilleger\\GeoGouv\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Cvilleger\\Test\\GeoGouv\\": "tests/"
        }
    }
}
